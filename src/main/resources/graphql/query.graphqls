type Query {
    person(id: ID): Person
    personByNameAndDateOfBirth(name: String, dateOfBirth: String): Person
    title(id: ID): Title
    titleByNameAndReleased(name: String, released: Int): Title
    genre(id: ID): Genre
    genreByName(name: String): Genre
    award(id: ID): Award
    awardByName(name: String): Award
    awardOrganisation(id: ID): AwardOrganisation
    awardOrganisationByName(name: String): AwardOrganisation
}